{
  "private": true,
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "homepage": "https://yunweb.github.io/crypto-zoo",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/plugin-esbuild": "^1.1.0",
    "@umijs/preset-react": "1.x",
    "bignumber.js": "^9.0.1",
    "ethers": "^5.0.26",
    "gh-pages": "^3.1.0",
    "rekv": "^1.2.2",
    "umi": "^3.3.5",
    "web3": "^1.3.1",
    "web3-utils": "^1.3.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/test": "^3.3.5",
    "husky": "^4.3.8",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
